{
  "name": "jasonmichels/framework",
  "description": "GroundworkPHP is a PHP 7 micro framework",
  "keywords": ["groundworkphp", "framework", "php", "php7"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jason Michels",
      "email": "michelsja@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/http-foundation": "^3.0",
    "nikic/fast-route": "^0.7.0"
  },
  "require-dev": {
    "phing/phing": "^2.13",
    "phpunit/phpunit": "^5.1",
    "mockery/mockery": "0.9.*",
    "halleck45/php-metrics": "dev-master",
    "squizlabs/php_codesniffer": "dev-master",
    "sebastian/phpcpd": "*",
    "phpmd/phpmd" : "@stable"
  },
  "autoload": {
    "psr-4": {
      "GroundworkPHP\\Framework\\": "src",
      "GroundworkPHP\\Framework\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev"
}
